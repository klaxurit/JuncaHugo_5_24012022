		{% if _session["FLASH_MESSAGES"]["comment"]["type"] is same as("error") %}
			<div class="alert alert-danger" role="alert">
				{{ _session["FLASH_MESSAGES"]["comment"]["message"] }}
			</div>
		{% endif %}
	{% endif %}
